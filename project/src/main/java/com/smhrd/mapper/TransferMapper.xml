<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smhrd.mapper.TransferMapper">


	<select id="select" resultType="TransferDTO">
		select * from transfer_tb order by reg_date desc
	</select>
	
	<select id="show" parameterType="Bigdecimal"
		resultType="TransferDTO">
		select * from transfer_tb where
		transfer_num=#{transfer_num}
	</select>
</mapper>